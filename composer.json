{
  "name": "syspons/laravel-sheetable",
  "description": "Laravel Helper for spreadsheet im-/export",
  "license": "MIT",
  "authors": [
      {
          "name": "Andreas Jabusch",
          "email": "andreas.jabusch@syspons.com"
      }
  ],
  "minimum-stability": "dev",
  "autoload" : {
      "psr-4" : {
          "syspons\\Sheetable\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "syspons\\Sheetable\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "syspons\\Sheetable\\SheetableServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "haydenpierce/class-finder": "dev-master"
  }
}
