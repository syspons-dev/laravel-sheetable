{
  "name": "syspons/laravel-sheetable",
  "description": "Laravel Helper for spreadsheet im-/export",
  "license": "MIT",
  "version": "1.0.2",
  "authors": [
      {
          "name": "Andreas Jabusch",
          "email": "andreas.jabusch@syspons.com"
      }
  ],

  "autoload" : {
      "psr-4" : {
          "Syspons\\Sheetable\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Syspons\\Sheetable\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "Syspons\\Sheetable\\SheetableServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "php": "^8.0",
    "haydenpierce/class-finder": "^0.4.3",
    "barryvdh/laravel-ide-helper": "2.8.2",
    "maatwebsite/excel": "4.0.x-dev"
  }
}
